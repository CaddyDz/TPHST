        "laravel-mix": "^2.0",
        "lodash": "^4.17.11",
        "owl.carousel": "^2.3.4",
        "popper.js": "^1.12",
        "vue": "^2.5.7"
    },
    "dependencies": {
        "@webwingscz/googlefont-montserrat": "^1.0.5",
        "bootstrap-validator": "^0.8.1",
        "font-awesome": "^4.5.0",
        "glyphicons-halflings": "^1.9.1",
        "jquery.stellar": "^0.6.2",
        "magnific-popup": "^1.1.0",
        "modernizr": "^3.6.0",
        "venobox": "^1.7.0"
    }
